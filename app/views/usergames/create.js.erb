scdemo=100;
maxind=0;
$('#playid').text('<%= @usergame.name%>')
// $('#playid').text('string2')
for (var i = 0; i < scr.length; i++)
{
	if (scr[i]<scdemo)
	{
		maxind=i;
		scdemo=scr[i];
	}
}
if (scr[0]==0&scr[3]==0&&scr[4]==0)
{$('#emoid').text('8')}
else
{$('#emoid').text(maxind.toString())}
